function repeatStringNumTimes(str, num) {
  // repeat after me
  return str;
}

repeatStringNumTimes("abc", 3);


function repeatStringNumTimes(str, num) {
var repeat = String(str);
  if (num < 0) {
    return "";
  } else {

for (var i=1; i < num; i++) {
  str = str.concat(repeat);
}
  return str;
    }
}

repeatStringNumTimes("abc", 3);
